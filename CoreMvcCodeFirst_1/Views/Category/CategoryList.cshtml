@model List<Category>

<table class="table table-hover table-primary">


    <thead>
        <tr>
            <th>Id</th>
            <th>Kategori Ismi</th>
            <th>Aciklama</th>
            <th>Guncelle</th>
            <th>Sil</th>
        </tr>



    </thead>


    <tbody>
        @foreach(Category item in Model)
        {
           <tr>

               <td>@item.Id</td>
               <td>@item.CategoryName</td>
               <td>@item.Description</td>
               <td></td>
               <td></td>

           </tr>
        }
            



     </tbody>


</table>